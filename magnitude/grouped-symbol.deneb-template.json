    }
  },
  "scales": [
    {
      "name": "scaleY",
      "type": "band",
      "domain": {
        "data": "dataset",
        "field": "__0__"
      },
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "scaleY",
      "orient": "left",
      "offset": {
        "signal": "dotRadius*2"
      },
      "labelFontSize": {
        "signal": "labelFontSize"
      },
      "labelColor": {
        "signal": "labelColor"
      },
      "domain": false,
      "ticks": false
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {"data": "dataset"},
      "encode": {
        "update": {
          "x": {
            "signal": "floor((datum['__2__']-1)/rowsPerGroup)*dotDistance"
          },
          "y": {
            "scale": "scaleY",
            "field": "__0__",
            "offset": {
              "signal": "bandwidth('scaleY')*1/(rowsPerGroup+1)+ (datum['__2__']-floor(datum['__2__']/rowsPerGroup)*rowsPerGroup) * dotDistance"
            }
          },
          "size": {
            "signal": "dotRadius*dotRadius*PI*2"
          },
          "stroke": {
            "signal": "'grey'"
          },
          "strokeWidth": {
            "signal": "1"
          },
          "fill": {
            "signal": "datum['__1__'] ? barColor1 : barColor2"
          }
        }
      }
    }
  ]
