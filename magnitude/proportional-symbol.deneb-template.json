{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "usermeta": {
    "deneb": {
      "build": "1.6.2.1",
      "metaVersion": 1,
      "provider": "vega",
      "providerVersion": "5.26.1"
    },
    "interactivity": {
      "tooltip": true,
      "contextMenu": true,
      "selection": true,
      "highlight": true,
      "dataPointLimit": 50
    },
    "information": {
      "name": "Magnitude - Proportional Symbol Chart",
      "description": "To show variations when fine difference are not so important",
      "author": "Andrzej Leszkiewicz",
      "uuid": "d7f11244-8198-46ff-85db-a74c9283b3c1",
      "generated": "2024-02-15T05:10:36.949Z",
      "previewImageBase64PNG": "data:image/png;base64,"
    },
    "dataset": [
      {
        "key": "__0__",
        "name": "Category",
        "description": "Category (bubble) name",
        "type": "text",
        "kind": "column"
      },
      {
        "key": "__1__",
        "name": "Value",
        "description": "Value (bubble are size)",
        "type": "numeric",
        "kind": "column"
      }
    ],
    "Project": "Deneb Vega Templates by Andrzej Leszkiewicz",
    "Github": "https://github.com/avatorl/Deneb-Vega-Templates/",
    "Copyright": "Andrzej Leszkiewicz",
    "License": "MIT Expat License (https://en.wikipedia.org/wiki/MIT_License)"
  },
  "config": {},
  "description": "Magnitude - Proportional Symbol Chart - To show variations when fine difference are not so important.",
  "padding": 15,
  "autosize": "fit",
  "background": "white",
  "signals": [
    {
      "name": "width",
      "value": 400,
      "update": "containerSize()[0]-30"
    },
    {
      "name": "height",
      "value": 400,
      "update": "containerSize()[1]-30"
    },
    {
      "name": "titleColor",
      "value": "#333333"
    },
    {
      "name": "color1",
      "init": "pbiColor(0)"
    },
    {
      "name": "labelColor",
      "value": "#666666"
    },
    {
      "name": "titleFontSize",
      "value": 24
    },
    {
      "name": "labelFontSize",
      "value": 24
    },
    {
      "name": "axisLabelFontSize",
      "value": 14
    },
    {
      "name": "bubbleOpacity",
      "value": 0.6
    },
    {
      "name": "bubbleStrokeWidth",
      "value": 1
    },
    {
      "name": "cx",
      "update": "width / 2"
    },
    {
      "name": "cy",
      "update": "height / 2"
    },
    {"name": "gravityX", "value": 0.2},
    {"name": "gravityY", "value": 0.1}
  ],
  "data": [{"name": "dataset"}],
  "scales": [
    {
      "name": "scaleSize",
      "type": "linear",
      "domain": {
        "data": "dataset",
        "field": "__1__"
      },
      "range": [0, 15000]
    }
  ],
  "title": {
    "text": "Proportional Symbol Chart",
    "anchor": "start",
    "offset": 10,
    "color": {"signal": "titleColor"},
    "fontSize": {
      "signal": "titleFontSize"
    },
    "subtitle": "To show size comparisons (when there are big variations)",
    "subtitlePadding": 10,
    "subtitleFontSize": {"signal": "16"}
  },
  "axes": [],
  "marks": [
    {
      "name": "symbol-bubbles",
      "type": "symbol",
      "from": {"data": "dataset"},
      "encode": {
        "enter": {
          "xfocus": {"signal": "cx"},
          "yfocus": {"signal": "cy"},
          "size": {
            "signal": "datum['__1__']",
            "scale": "scaleSize"
          },
          "fill": {"signal": "color1"},
          "opacity": {
            "signal": "bubbleOpacity"
          },
          "stroke": {
            "signal": "color1"
          },
          "strokeWidth": {
            "signal": "bubbleStrokeWidth"
          }
        }
      },
      "transform": [
        {
          "type": "force",
          "iterations": 100,
          "static": false,
          "forces": [
            {
              "force": "collide",
              "iterations": 2,
              "radius": {
                "expr": "sqrt(datum.size) / 2 + bubbleStrokeWidth"
              }
            },
            {
              "force": "center",
              "x": {"signal": "cx"},
              "y": {"signal": "cy"}
            },
            {
              "force": "x",
              "x": "xfocus",
              "strength": {
                "signal": "gravityX"
              }
            },
            {
              "force": "y",
              "y": "yfocus",
              "strength": {
                "signal": "gravityY"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "text-labels",
      "type": "text",
      "from": {
        "data": "symbol-bubbles"
      },
      "encode": {
        "enter": {
          "text": {
            "field": "datum['__0__']"
          },
          "align": {"value": "center"},
          "baseline": {
            "value": "middle"
          },
          "fontSize": {
            "signal": "labelFontSize"
          },
          "fontWeight": {
            "value": "normal"
          },
          "fill": {
            "signal": "labelColor"
          }
        },
        "update": {
          "x": {"field": "x"},
          "y": {"field": "y"}
        }
      },
      "transform": [
        {
          "type": "label",
          "anchor": [
            "left",
            "right",
            "top-left",
            "top-right",
            "bottom-left",
            "bottom-right"
          ],
          "padding": 0,
          "size": {
            "signal": "[width, height]"
          }
        }
      ]
    }
  ]
}
