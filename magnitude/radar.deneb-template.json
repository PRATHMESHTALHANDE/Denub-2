{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "usermeta": {
    "deneb": {
      "build": "1.6.2.1",
      "metaVersion": 1,
      "provider": "vega",
      "providerVersion": "5.26.1"
    },
    "interactivity": {
      "tooltip": true,
      "contextMenu": true,
      "selection": false,
      "highlight": false,
      "dataPointLimit": 50
    },
    "information": {
      "name": "Magnitude - Radar",
      "description": "To show values of multiple variables",
      "author": "Andrzej Leszkiewicz",
      "uuid": "53fdd0f0-be8f-487d-b856-664063dd2178",
      "generated": "2024-02-15T08:37:07.764Z",
      "previewImageBase64PNG": "data:image/png;base64,"
    },
    "dataset": [
      {
        "key": "__0__",
        "name": "Category",
        "description": "Variable name",
        "type": "text",
        "kind": "column"
      },
      {
        "key": "__1__",
        "name": "Value",
        "description": "Value",
        "type": "numeric",
        "kind": "column"
      }
    ],
    "Project": "Deneb Vega Templates by Andrzej Leszkiewicz",
    "Github": "https://github.com/avatorl/Deneb-Vega-Templates/",
    "Copyright": "Andrzej Leszkiewicz",
    "License": "MIT Expat License (https://en.wikipedia.org/wiki/MIT_License)",
    "Inspired By (chart types)": "FT Visual Vocabulary (https://ft.com/vocabulary)"
  },
  "config": {},
  "description": "Magnitude - Radar. To show values of multiple variables.",
  "padding": 15,
  "autosize": "fit",
  "background": "white",
  "signals": [
    {
      "name": "width",
      "value": 400,
      "update": "containerSize()[0]-30"
    },
    {
      "name": "height",
      "value": 400,
      "update": "containerSize()[1]-30"
    },
    {
      "name": "titleText",
      "value": "Radar"
    },
    {
      "name": "subTitleText",
      "value": "To show values of multiple variables"
    },
    {
      "name": "titleOffset",
      "description": "Blank space between the subtitle and the chart, px",
      "value": 20
    },
    {
      "name": "subtitlePadding",
      "description": "Blank space between the title and the subtitle, px",
      "value": 10
    },
    {
      "name": "titleColor",
      "value": "#333333"
    },
    {
      "name": "color1",
      "init": "pbiColor(0)"
    },
    {
      "name": "labelColor",
      "value": "#666666"
    },
    {
      "name": "titleFontSize",
      "value": 24
    },
    {
      "name": "subtitleFontSize",
      "value": 16
    },
    {
      "name": "labelAxisFontSize",
      "value": 24
    },
    {
      "name": "labelFontSize",
      "value": 20
    },
    {"name": "labelOffset", "value": 5},
    {
      "name": "lineStrokeWidth",
      "value": 3
    },
    {
      "name": "areaFillOpacity",
      "value": 0.5
    },
    {
      "name": "radius",
      "update": "width / 3"
    },
    {
      "name": "centerx",
      "update": "width / 2"
    },
    {
      "name": "centery",
      "update": "height / 2"
    }
  ],
  "data": [
    {"name": "dataset"},
    {
      "name": "data-categories",
      "source": "dataset",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["__0__"]
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "angular",
      "type": "point",
      "range": {"signal": "[-PI, PI]"},
      "padding": 0.5,
      "domain": {
        "data": "dataset",
        "field": "__0__"
      }
    },
    {
      "name": "radial",
      "type": "linear",
      "range": [
        {"signal": "0"},
        {"signal": "radius"}
      ],
      "zero": true,
      "nice": false,
      "domain": {
        "data": "dataset",
        "field": "__1__"
      },
      "domainMin": 0
    }
  ],
  "title": {
    "text": {"signal": "titleText"},
    "anchor": "start",
    "offset": {"signal": "titleOffset"},
    "color": {"signal": "titleColor"},
    "fontSize": {
      "signal": "titleFontSize"
    },
    "subtitle": {
      "signal": "subTitleText"
    },
    "subtitlePadding": {
      "signal": "subtitlePadding"
    },
    "subtitleFontSize": {
      "signal": "subtitleFontSize"
    }
  },
  "axes": [],
  "marks": [
    {
      "type": "line",
      "name": "line-category",
      "from": {"data": "dataset"},
      "zindex": 1,
      "encode": {
        "enter": {
          "interpolate": {
            "value": "linear-closed"
          },
          "x": {
            "signal": "centerx + scale('radial', datum['__1__']) * cos(scale('angular', datum['__0__']))"
          },
          "y": {
            "signal": "centery + scale('radial', datum['__1__']) * sin(scale('angular', datum['__0__']))"
          },
          "stroke": {
            "signal": "color1"
          },
          "strokeWidth": {
            "signal": "lineStrokeWidth"
          },
          "fill": {"signal": "color1"},
          "fillOpacity": {
            "signal": "areaFillOpacity"
          }
        }
      }
    },
    {
      "type": "text",
      "name": "value-text",
      "from": {"data": "line-category"},
      "encode": {
        "enter": {
          "x": {"signal": "datum.x"},
          "y": {"signal": "datum.y"},
          "text": {
            "signal": "datum['__1__']"
          },
          "align": {"value": "center"},
          "baseline": {
            "value": "middle"
          },
          "fill": {"value": "black"}
        }
      }
    },
    {
      "type": "rule",
      "name": "radial-grid",
      "from": {
        "data": "data-categories"
      },
      "zindex": 0,
      "encode": {
        "enter": {
          "x": {"signal": "centerx"},
          "y": {"signal": "centery"},
          "x2": {
            "signal": "centerx + radius * cos(scale('angular', datum['__0__']))"
          },
          "y2": {
            "signal": "centery + radius * sin(scale('angular', datum['__0__']))"
          },
          "stroke": {
            "value": "lightgray"
          },
          "strokeWidth": {"value": 1}
        }
      }
    },
    {
      "type": "text",
      "name": "Category-label",
      "from": {
        "data": "data-categories"
      },
      "zindex": 1,
      "encode": {
        "enter": {
          "x": {
            "signal": "centerx + (radius + 5) * cos(scale('angular', datum['__0__']))"
          },
          "y": {
            "signal": "centery + (radius + 5) * sin(scale('angular', datum['__0__']))"
          },
          "text": {"field": "__0__"},
          "align": [
            {
              "test": "abs(scale('angular', datum['__0__'])) > PI / 2",
              "value": "right"
            },
            {"value": "left"}
          ],
          "baseline": [
            {
              "test": "scale('angular', datum['__0__']) > 0",
              "value": "top"
            },
            {
              "test": "scale('angular', datum['__0__']) == 0",
              "value": "middle"
            },
            {"value": "bottom"}
          ],
          "fill": {
            "signal": "labelColor"
          },
          "fontSize": {
            "signal": "labelFontSize"
          }
        }
      }
    },
    {
      "type": "line",
      "name": "outer-line",
      "from": {"data": "radial-grid"},
      "encode": {
        "enter": {
          "interpolate": {
            "value": "linear-closed"
          },
          "x": {"field": "x2"},
          "y": {"field": "y2"},
          "stroke": {
            "value": "lightgray"
          },
          "strokeWidth": {"value": 1}
        }
      }
    }
  ]
}
