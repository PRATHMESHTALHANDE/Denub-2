        {"signal": "dataColor2"},
        {"signal": "dataColor3"},
        {"signal": "dataColor4"},
        {"signal": "dataColor5"}
      ]
    },
    {
      "name": "scaleGrey",
      "type": "ordinal",
      "domain": {
        "data": "data-category",
        "fields": ["__0__"]
      },
      "range": [
        {"signal": "'#666666'"},
        {"signal": "'#777777'"},
        {"signal": "'#888888'"},
        {"signal": "'#999999'"},
        {"signal": "'#AAAAAA'"}
      ]
    }
  ],
  "title": {
