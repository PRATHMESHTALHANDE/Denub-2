          },
          "cornerRadius": {
            "signal": "0"
          },
          "stroke": {
            "signal": "'white'"
          },
          "strokeWidth": {"value": 2}
        }
      }
    },
    {
      "type": "text",
      "from": {"data": "data-category"},
      "zindex": 1,
      "encode": {
        "enter": {
          "fill": {
            "signal": "datum['__0__'] == 'Dragonfruit' ? scale('scaleColor', datum['__0__']) : scale('scaleGrey', datum['__0__'])"
          },
          "x": {"signal": "0"},
          "y": {"signal": "0"},
          "theta": {
