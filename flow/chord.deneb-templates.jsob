    }
  ],
  "data": [
    {
      "name": "dataset",
      "transform": [
        {
          "type": "filter",
          "expr": "datum['__0__']!=datum['__1__']"
        }
      ]
    },
    {
      "name": "data-nodes",
      "source": "dataset",
      "transform": [
        {
          "type": "fold",
          "fields": ["__0__", "__1__"],
          "as": ["key", "value"]
        },
        {
          "type": "formula",
          "expr": "datum.key == '__0__' ? datum['__0__']+'-'+datum['__1__'] : datum['__1__']+'-'+datum['__0__']",
          "as": "link"
        }
      ]
    },
    {
      "name": "data-category",
      "source": "dataset",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["__0__"],
          "fields": ["__2__"],
          "ops": ["sum"],
          "as": ["__2__"]
        },
        {
          "type": "pie",
          "field": "__2__",
          "startAngle": {"signal": "0"},
          "endAngle": {
            "signal": "2*PI"
          },
          "sort": {"signal": "false"}
        }
      ]
    },
    {
      "name": "data-nodes-source",
      "source": "data-nodes",
      "transform": [
        {
          "type": "filter",
          "expr": "datum.key == '__0__'"
        },
        {
          "type": "collect",
          "sort": {
            "field": "__0__",
            "order": "ascending"
          }
        },
        {
          "type": "pie",
          "field": "__2__",
          "startAngle": {"signal": "0"},
          "endAngle": {
            "signal": "2*PI"
          },
          "sort": {"signal": "false"}
        }
      ]
    },
    {
      "name": "data-nodes-target",
      "source": "data-nodes",
      "transform": [
        {
          "type": "filter",
          "expr": "datum.key == '__1__'"
        },
        {
          "type": "collect",
          "sort": {
            "field": "__0__",
            "order": "ascending"
          }
        },
        {
          "type": "pie",
          "field": "__2__",
          "startAngle": {"signal": "0"},
          "endAngle": {
            "signal": "2*PI"
          },
          "sort": {"signal": "false"}
        }
      ]
    },
    {
      "name": "data-links",
      "source": "data-nodes-source",
      "transform": [
        {
          "type": "filter",
          "expr": "datum['__row__'] <= 99"
        },
        {
          "type": "lookup",
          "from": "data-nodes-target",
          "key": "link",
          "fields": ["link"],
          "as": ["__1__"]
        },
        {
          "type": "formula",
          "expr": "'M '+round(radius*cos(datum.startAngle-PI/2))+' '+round(radius*sin(datum.startAngle-PI/2))+' A '+radius+' '+radius+' 0 0 1 '+round(radius*cos(datum.endAngle-PI/2))+' '+round(radius*sin(datum.endAngle-PI/2))+' Q 0 0 '+round(radius*cos(datum['__1__'].startAngle-PI/2))+' '+round(radius*sin(datum['__1__'].startAngle-PI/2))+' A '+radius+' '+radius+' 0 0 1 '+round(radius*cos(datum['__1__'].endAngle-PI/2))+' '+round(radius*sin(datum['__1__'].endAngle-PI/2))+' Q 0 0 '+round(radius*cos(datum.startAngle-PI/2))+' '+round(radius*sin(datum.startAngle-PI/2))+'Z'",
          "as": "path"
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "scaleColor",
      "type": "ordinal",
      "domain": {
        "data": "data-category",
        "fields": ["__0__"]
      },
      "range": [
