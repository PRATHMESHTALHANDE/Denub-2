            "signal": "datum['__0__'] == 'Dragonfruit' ? scale('scaleColor', datum['__0__']) : scale('scaleGrey', datum['__0__'])"
          },
          "opacity": {
            "signal": "datum['__0__'] == 'Dragonfruit' ? 0.8 : 0.5"
          },
          "stroke": {"value": "white"},
          "strokeWidth": {"value": 2}
        },
        "update": {
          "path": {"field": "path"}
        }
