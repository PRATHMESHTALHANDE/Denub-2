          },
          "radius": {
            "signal": "radius*1.24"
          },
          "align": {
            "signal": "(datum.startAngle+datum.endAngle)/2 > PI ? 'right' : 'left'"
          },
          "baseline": {
            "signal": "(datum.startAngle+datum.endAngle)/2 < PI/2 || ((datum.startAngle+datum.endAngle)/2) > 1.5*PI ? 'bottom' : 'top'"
          },
