      "author": "Andrzej Leszkiewicz",
      "uuid": "b1fdda6f-b78a-45fd-8ce1-995060d4a28b",
      "generated": "2024-02-09T21:31:58.844Z",
      "previewImageBase64PNG": "data:image/png;base64,"
    },
    "dataset": [
      {
        "key": "__0__",
        "name": "Node 1",
        "description": "Node 1 name",
        "type": "text",
        "kind": "column"
      },
      {
        "key": "__1__",
        "name": "Node 2",
        "description": "Node 2 name",
        "type": "text",
        "kind": "column"
      },
      {
        "key": "__2__",
        "name": "source",
        "description": "Node 1 id",
        "type": "numeric",
        "kind": "column"
      },
      {
        "key": "__3__",
        "name": "target",
        "description": "Node 2 id",
        "type": "numeric",
        "kind": "column"
      }
    ],
    "Project": "Deneb Vega Templates by Andrzej Leszkiewicz",
    "Github": "https://github.com/avatorl/Deneb-Vega-Templates/",
