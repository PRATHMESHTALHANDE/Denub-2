{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "usermeta": {
    "deneb": {
      "build": "1.5.1.0",
      "metaVersion": 1,
      "provider": "vega",
      "providerVersion": "5.23.0"
    },
    "interactivity": {
      "tooltip": true,
      "contextMenu": true,
      "selection": true,
      "highlight": true,
      "dataPointLimit": 50
    },
    "information": {
      "name": " Gridplot",
      "description": "To show % value (whole numbers)",
      "author": "Andrzej Leszkiewicz",
      "uuid": "67c2a886-7d69-4409-a61a-796f061ba83e",
      "generated": "2023-09-16T23:09:19.507Z",
      "previewImageBase64PNG": "data:image/png;base64,"
    },
    "dataset": [
      {
        "key": "__0__",
        "name": "Category",
        "description": "",
        "type": "text",
        "kind": "column"
      },
      {
        "key": "__1__",
        "name": "Value",
        "description": "",
        "type": "numeric",
        "kind": "column"
      }
    ],
    "Project": "Deneb Vega Templates by Andrzej Leszkiewicz",
    "Github": "https://github.com/avatorl/Deneb-Vega-Templates/",
    "Author": "Andrzej Leszkewicz",
    "License": "CC BY 4.0 (Share and Adapt with Attribution)",
    "Inspired By (chart types)": "FT Visual Vocabulary (https://ft.com/vocabulary)"
  },
  "config": {},
  "description": "Part-to-Whole - Gridplot. To show % value (whole numbers).",
  "padding": 15,
  "autosize": "fit",
  "background": "white",
  "signals": [
    {
      "name": "width",
      "value": 400,
      "update": "containerSize()[0]-30"
    },
    {
      "name": "height",
      "value": 400,
      "update": "containerSize()[1]-30"
    },
    {
      "name": "titleText",
      "value": "Gridplot"
    },
    {
      "name": "subTitleText",
      "value": "To show % value (whole numbers)"
    },
    {
      "name": "titleOffset",
      "description": "Blank space between the subtitle and the chart, px",
      "value": 20
    },
    {
      "name": "subtitlePadding",
      "description": "Blank space between the title and the subtitle, px",
      "value": 10
    },
    {
      "name": "subtitleFontSize",
      "value": 16
    },
    {
      "name": "titleColor",
      "value": "#333333"
    },
    {
      "name": "barColor1",
      "init": "pbiColor(0)"
    },
    {
      "name": "barColor2",
      "init": "pbiColor(2)"
    },
    {
      "name": "labelColor",
      "value": "#666666"
    },
    {
      "name": "titleFontSize",
      "value": 24
    },
    {
      "name": "labelFontSize",
      "value": 18
    },
    {"name": "labelOffset", "value": 5}
  ],
  "data": [
    {"name": "dataset"},
    {
      "name": "grid",
      "transform": [
        {
          "type": "sequence",
          "start": 0,
          "stop": 100
        },
        {
          "type": "formula",
          "expr": "floor(datum.data/10)",
          "as": "x"
        },
        {
          "type": "formula",
          "expr": "datum.data-datum.x*10",
          "as": "y"
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "scaleX",
      "type": "band",
      "domain": {
        "data": "grid",
        "field": "x"
      },
      "range": [{"signal": "width"}, 0]
    },
    {
      "name": "scaleY",
      "type": "band",
      "domain": {
        "data": "grid",
        "field": "y"
      },
      "range": [0, {"signal": "height"}]
    }
  ],
  "title": {
    "text": {"signal": "titleText"},
    "anchor": "start",
    "offset": {"signal": "titleOffset"},
    "color": {"signal": "titleColor"},
    "fontSize": {
      "signal": "titleFontSize"
    },
    "subtitle": {
      "signal": "subTitleText"
    },
    "subtitlePadding": {
      "signal": "subtitlePadding"
    },
    "subtitleFontSize": {
      "signal": "subtitleFontSize"
    }
  },
  "axes": [],
  "marks": [
    {
      "name": "rect-main",
      "type": "rect",
      "from": {"data": "grid"},
      "encode": {
        "enter": {
          "x": {
            "scale": "scaleX",
            "field": "y"
          },
          "y": {
            "scale": "scaleY",
            "field": "x"
          },
          "width": {
            "scale": "scaleX",
            "band": 1
          },
          "height": {
            "scale": "scaleY",
            "band": 1
          },
          "stroke": {
            "signal": "'white'"
          },
          "fill": {
            "signal": "datum.data>=(1-data('dataset')[0]['__1__'])*100?barColor1:'#CCCCCC'"
          }
        }
      }
    },
    {
      "type": "text",
      "zindex": 1,
      "from": {"data": "dataset"},
      "encode": {
        "enter": {
          "text": {
            "signal": "format(data('dataset')[0]['__1__'],'0.0%')"
          },
          "fontSize": {"signal": "100"},
          "fill": {
            "signal": "'#333333'"
          },
          "opacity": {"signal": "1"},
          "x": {"signal": "width/2"},
          "y": {"signal": "height/2"},
          "align": {
            "signal": "'center'"
          },
          "baseline": {
            "signal": "'middle'"
          }
        }
      }
    }
  ]
}
