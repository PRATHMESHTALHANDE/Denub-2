{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "usermeta": {
    "deneb": {
      "build": "1.6.2.1",
      "metaVersion": 1,
      "provider": "vega",
      "providerVersion": "5.26.1"
    },
    "interactivity": {
      "tooltip": true,
      "contextMenu": true,
      "selection": true,
      "highlight": true,
      "dataPointLimit": 50
    },
    "information": {
      "name": "Part-to-Whole - Arc (Parliament Diagram)",
      "description": "To visualize parliamentary composition by number of seats",
      "author": "Andrzej Leszkiewicz",
      "uuid": "4f78a15c-3a88-4afd-affe-2fdb67d9e34c",
      "generated": "2024-02-16T22:24:03.035Z",
      "previewImageBase64PNG": "data:image/png;base64,"
    },
    "dataset": [
      {
        "key": "__0__",
        "name": "Index",
        "description": "",
        "type": "numeric",
        "kind": "column"
      },
      {
        "key": "__1__",
        "name": "Party",
        "description": "",
        "type": "text",
        "kind": "column"
      },
      {
        "key": "__2__",
        "name": "Seats",
        "description": "",
        "type": "numeric",
        "kind": "column"
      }
    ],
    "Project": "Deneb Vega Templates by Andrzej Leszkiewicz",
    "Github": "https://github.com/avatorl/Deneb-Vega-Templates/",
    "Copyright": "Andrzej Leszkiewicz",
    "License": "MIT Expat License (https://en.wikipedia.org/wiki/MIT_License)"
  },
  "config": {},
  "description": "Part-to-Whole - Arc (Parliament Diagram). To visualize parliamentary composition by number of seats.",
  "padding": 15,
  "autosize": "fit",
  "background": "white",
  "signals": [
    {
      "name": "width",
      "value": 400,
      "description": "based on container width",
      "update": "containerSize()[0]-30"
    },
    {
      "name": "height",
      "value": 400,
      "description": "based on container height",
      "update": "containerSize()[1]-30"
    },
    {
      "name": "radius",
      "description": "radius of the outer ring",
      "update": "width*0.5"
    },
    {
      "name": "arcSeatsCount",
      "description": "number of seats (dots), whole number",
      "update": "seatsCountx[1]"
    },
    {
      "name": "seatWidthPct",
      "description": "seat (dot) width",
      "value": 1.05
    },
    {
      "name": "arcInnerSpace",
      "description": "Inner radius of the arc; edit this value until total number of seats is correct, [0, 1]",
      "value": 0.65
    },
    {
      "name": "arcAngle",
      "description": "[start, end], where Start=[0, 2*PI], End=[0, 2*PI]; [1.5*PI, 0.5*PI] - half circle",
      "update": "[1.5*PI, 0.5*PI]"
    },
    {
      "name": "arcAngleWidth",
      "description": "Arc angle width in Radians",
      "update": "arcAngle[0]-arcAngle[1]"
    },
    {
      "name": "radiusInner",
      "description": "radius of the inner ring",
      "update": "radius*arcInnerSpace"
    },
    {
      "name": "arcAreaOuter",
      "description": "arc area (with white spaces between dots)",
      "update": "PI*radius*radius*arcAngleWidth/(2*PI)"
    },
    {
      "name": "arcAreaInner",
      "description": "arc area (inner circle / empty space)",
      "update": "PI*radiusInner*radiusInner*arcAngleWidth/(2*PI)"
    },
    {
      "name": "arcArea",
      "description": "arc area (excluding inner empty space))",
      "update": "arcAreaOuter-arcAreaInner"
    },
    {
      "name": "seatPlaceArea",
      "description": "seat (dot and surrounding white space) area",
      "update": "arcArea/arcSeatsCount"
    },
    {
      "name": "seatPlaceWidth",
      "description": "seat place (dot and surrounding white space) width",
      "update": "sqrt(seatPlaceArea/PI)*2"
    },
    {
      "name": "seatRadius",
      "description": "seat (dot) radius",
      "update": "seatPlaceWidth*seatWidthPct/2"
    },
    {
      "name": "r1Count",
      "description": "seats in a row",
      "update": "floor((radius*arcAngleWidth)/seatPlaceWidth)"
    },
    {
      "name": "r2Count",
      "description": "seats in a row",
      "update": "floor(((radius-seatPlaceWidth*1)*arcAngleWidth)/seatPlaceWidth)"
    },
    {
      "name": "r3Count",
      "description": "seats in a row",
      "update": "floor(((radius-seatPlaceWidth*2)*arcAngleWidth)/seatPlaceWidth)"
    },
    {
      "name": "r4Count",
      "description": "seats in a row",
      "update": "floor(((radius-seatPlaceWidth*3)*arcAngleWidth)/seatPlaceWidth)"
    },
    {
      "name": "r5Count",
      "description": "seats in a row",
      "update": "floor(((radius-seatPlaceWidth*4)*arcAngleWidth)/seatPlaceWidth)"
    },
    {
      "name": "titleText",
      "value": "Arc (Parliament Diagram)"
    },
    {
      "name": "subTitleText",
      "value": "To show parliamentary composition by number of seats"
    },
    {
      "name": "titleColor",
      "value": "#333333"
    },
    {
      "name": "titleOffset",
      "description": "Blank space between the subtitle and the chart, px",
      "value": 20
    },
    {
      "name": "subtitlePadding",
      "description": "Blank space between the title and the subtitle, px",
      "value": 10
    },
    {
      "name": "subtitleFontSize",
      "value": 16
    },
    {
      "name": "color1",
      "init": "pbiColor(0)"
    },
    {
      "name": "color2",
      "init": "pbiColor(2)"
    },
    {
      "name": "labelColor",
      "value": "#666666"
    },
    {
      "name": "titleFontSize",
      "value": "24"
    },
    {
      "name": "seatLabelFontSize",
      "value": "8"
    }
  ],
  "data": [
    {
      "name": "dataset",
      "transform": [
        {
          "type": "stack",
          "groupby": ["party"],
          "field": "__2__",
          "sort": {
            "field": "__0__",
            "order": "ascending"
          }
        },
        {
          "type": "extent",
          "field": "y1",
          "signal": "seatsCountx"
        },
        {
          "type": "formula",
          "expr": "datum.y1+1",
          "as": "y"
        }
      ]
    },
    {
      "name": "data-r1",
      "transform": [
        {
          "type": "sequence",
          "start": 1,
          "stop": {"signal": "r1Count"},
          "as": "id"
        },
        {
          "type": "formula",
          "expr": "radius",
          "as": "r"
        },
        {
          "type": "formula",
          "expr": "r1Count",
          "as": "count"
        }
      ]
    },
    {
      "name": "data-r2",
      "transform": [
        {
          "type": "sequence",
          "start": 1,
          "stop": {"signal": "r2Count"},
          "as": "id"
        },
        {
          "type": "formula",
          "expr": "radius-seatPlaceWidth*1",
          "as": "r"
        },
        {
          "type": "formula",
          "expr": "r2Count",
          "as": "count"
        }
      ]
    },
    {
      "name": "data-r3",
      "transform": [
        {
          "type": "sequence",
          "start": 1,
          "stop": {"signal": "r3Count"},
          "as": "id"
        },
        {
          "type": "formula",
          "expr": "radius-seatPlaceWidth*2",
          "as": "r"
        },
        {
          "type": "formula",
          "expr": "r3Count",
          "as": "count"
        }
      ]
    },
    {
      "name": "data-r4",
      "transform": [
        {
          "type": "sequence",
          "start": 1,
          "stop": {"signal": "r4Count"},
          "as": "id"
        },
        {
          "type": "formula",
          "expr": "radius-seatPlaceWidth*3",
          "as": "r"
        },
        {
          "type": "formula",
          "expr": "r4Count",
          "as": "count"
        }
      ]
    },
    {
      "name": "data-r5",
      "transform": [
        {
          "type": "sequence",
          "start": 1,
          "stop": {"signal": "r5Count"},
          "as": "id"
        },
        {
          "type": "formula",
          "expr": "radius-seatPlaceWidth*4",
          "as": "r"
        },
        {
          "type": "formula",
          "expr": "r5Count",
          "as": "count"
        }
      ]
    },
    {
      "name": "data-r-all",
      "source": [
        "data-r1",
        "data-r2",
        "data-r3",
        "data-r4",
        "data-r5"
      ],
      "transform": [
        {
          "type": "collect",
          "sort": {
            "field": ["id"],
            "order": ["ascending"]
          }
        },
        {
          "type": "window",
          "ops": ["row_number"],
          "as": ["index"]
        },
        {
          "type": "formula",
          "expr": "datum.id/datum.count",
          "as": "idx"
        },
        {
          "type": "collect",
          "sort": {
            "field": ["idx"],
            "order": ["ascending"]
          }
        },
        {
          "type": "window",
          "ops": ["row_number"],
          "as": ["index"]
        },
        {
          "type": "formula",
          "expr": "scale('scaleColor',datum.index)",
          "as": "color"
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "scaleX",
      "type": "linear",
      "domain": {
        "data": "dataset",
        "field": "x"
      },
      "range": "width"
    },
    {
      "name": "scaleY",
      "type": "linear",
      "domain": {
        "data": "dataset",
        "field": "y"
      },
      "range": [
        {"signal": "height*0.5"},
        {"signal": "height*1"}
      ]
    },
    {
      "name": "scaleColor",
      "type": "threshold",
      "domain": [66, 114, 126],
      "range": [
        {"signal": "pbiColor(0)"},
        {"signal": "pbiColor(2)"},
        {"signal": "'grey'"}
      ]
    }
  ],
  "title": {
    "text": {"signal": "titleText"},
    "anchor": "start",
    "offset": {"signal": "titleOffset"},
    "color": {"signal": "titleColor"},
    "fontSize": {
      "signal": "titleFontSize"
    },
    "subtitle": {
      "signal": "subTitleText"
    },
    "subtitlePadding": {
      "signal": "subtitlePadding"
    },
    "subtitleFontSize": {
      "signal": "subtitleFontSize"
    }
  },
  "axes": [],
  "marks": [
    {
      "name": "text-seat",
      "comment": "using invisible text mark to calcualte bounds.x1 and bounds.y1, because text mark has theta property",
      "type": "text",
      "from": {"data": "data-r-all"},
      "zindex": 1,
      "encode": {
        "enter": {
          "x": {"signal": "width/2"},
          "y": {
            "signal": "height/2+150"
          },
          "align": {"value": "center"},
          "baseline": {
            "value": "middle"
          },
          "radius": {
            "signal": "datum.r"
          },
          "theta": {
            "signal": "arcAngle[0]+arcAngleWidth*(datum.id/datum.count)"
          },
          "fontSize": {
            "signal": "seatLabelFontSize"
          },
          "text": {
            "signal": "datum.index"
          },
          "fill": {"signal": "'white'"}
        }
      }
    },
    {
      "name": "symbol-seat",
      "type": "symbol",
      "shape": "circle",
      "from": {"data": "text-seat"},
      "zindex": 0,
      "encode": {
        "enter": {
          "x": {
            "signal": "(datum.bounds.x1+datum.bounds.x2)/2"
          },
          "y": {
            "signal": "(datum.bounds.y1+datum.bounds.y2)/2"
          },
          "size": {
            "signal": "PI*seatRadius*seatRadius"
          },
          "fill": {
            "signal": "datum.datum['color']"
          }
        }
      }
    },
    {
      "name": "text-total-seats",
      "type": "text",
      "zindex": 1,
      "encode": {
        "enter": {
          "x": {"signal": "width/2"},
          "y": {
            "signal": "height/2+100"
          },
          "align": {"value": "center"},
          "baseline": {
            "value": "middle"
          },
          "radius": {
            "signal": "datum.r"
          },
          "theta": {
            "signal": "arcAngleWidth[0]+arcAngleWidth*datum.id/datum.count"
          },
          "fontSize": {"value": 40},
          "fill": {"signal": "'grey'"},
          "text": {
            "signal": "length(data('data-r-all'))+' seats'"
          }
        }
      }
    }
  ]
}
